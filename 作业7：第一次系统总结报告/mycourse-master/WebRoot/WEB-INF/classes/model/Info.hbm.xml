<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="model.Info" table="Info" catalog="db_courseselection">
        <composite-id name="infoId" class="model.InfoId">
        	<key-property name="sid" type="java.lang.Integer">
        		<column name="sid"></column>
        	</key-property>
        	<key-property name="cid" type="java.lang.Integer">
        		<column name="cid"></column>
        	</key-property>
        	<key-property name="tid" type="java.lang.Integer">
        		<column name="tid"></column>
        	</key-property>
        	
        </composite-id>
       
    </class>
</hibernate-mapping>
